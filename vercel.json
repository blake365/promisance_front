{
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/index.html"
		}
	],
	"crons": [
		{
			"path": "/api/cron/turns",
			"schedule": "*/1 * * * *"
		},
		{
			"path": "/api/cron/thirty",
			"schedule": "*/30 * * * *"
		},
		{
			"path": "/api/cron/hour",
			"schedule": "0 * * * *"
		},
		{
			"path": "/api/cron/daily",
			"schedule": "0 20 * * *"
		}
	]
}
