{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ], 
  "crons": [
    {
      "path": "/api/cron/turns",
      "schedule": "*/12 * * * *"
    },
    {
      "path": "/api/cron/thirty",
      "schedule": "*/30 * * * *"
    },
    {
      "path":"/api/cron/hour",
      "schedule": "1 * * * *"
    },
    {
      "path":"/api/cron/daily",
      "schedule": "0 0 * * *" 
    },
    {
      "path":"/api/cron/aid",
      "schedule": "0 */5 * * *"
    }
  ]
}